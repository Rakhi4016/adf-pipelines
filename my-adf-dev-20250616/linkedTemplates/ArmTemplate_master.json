{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "my-adf-dev-20250616"
		},
		"AzureSqlLS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlLS'"
		},
		"MySqlLS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'MySqlLS'"
		},
		"AzureSqlLS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "my-sql-dev.database.windows.net"
		},
		"AzureSqlLS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "my-sql-dev"
		},
		"AzureSqlLS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "rakesh4016"
		},
		"Blue_Ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://blueadlsdev.dfs.core.windows.net/"
		},
		"MySqlLS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "localhost"
		},
		"MySqlLS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "loyal_services"
		},
		"MySqlLS_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "root"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "my-adf-dev-20250616_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlLS_password": {
						"value": "[parameters('AzureSqlLS_password')]"
					},
					"MySqlLS_password": {
						"value": "[parameters('MySqlLS_password')]"
					},
					"AzureSqlLS_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlLS_properties_typeProperties_server')]"
					},
					"AzureSqlLS_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlLS_properties_typeProperties_database')]"
					},
					"AzureSqlLS_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlLS_properties_typeProperties_userName')]"
					},
					"Blue_Ls_properties_typeProperties_url": {
						"value": "[parameters('Blue_Ls_properties_typeProperties_url')]"
					},
					"MySqlLS_properties_typeProperties_server": {
						"value": "[parameters('MySqlLS_properties_typeProperties_server')]"
					},
					"MySqlLS_properties_typeProperties_database": {
						"value": "[parameters('MySqlLS_properties_typeProperties_database')]"
					},
					"MySqlLS_properties_typeProperties_username": {
						"value": "[parameters('MySqlLS_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}